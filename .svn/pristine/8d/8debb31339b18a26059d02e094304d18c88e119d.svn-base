<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="id" name="id" th:value="${outWork.id}"  type="hidden">
<div class="form-group">	
								<label class="col-sm-3 control-label">申请编号：</label>
								<div class="col-sm-8">
									<input id="applyNo" readonly="readonly" name="applyNo" th:value="${outWork.applyNo}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">申请人：</label>
								<div class="col-sm-8">
									<input id="applyId"  readonly="readonly" name="applyId" th:value="${outWork.applyId}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">目的地：</label>
								<div class="col-sm-8">
									<input id="destination" name="destination" th:value="${outWork.destination}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">出发日：</label>
								<div class="col-sm-8">
									<input id="outDate" name="outDate" readonly="readonly" onClick="WdatePicker({dateFmt:'yyyy-MM-dd'})" th:value="${#calendars.format(outWork.outDate,'yyyy-MM-dd')}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">申请天数：</label>
								<div class="col-sm-8">
									<input id="applyDays" name="applyDays" th:value="${outWork.applyDays}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">对应客户：</label>
								<div class="col-sm-8">
									<input id="customerName" name="customerName" th:value="${outWork.customerName}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">对应项目：</label>
								<div class="col-sm-8">
									<input id="projectName" name="projectName" th:value="${outWork.projectName}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">预计费用：</label>
								<div class="col-sm-8">
									<input id="forecastAmount" name="forecastAmount" th:value="${outWork.forecastAmount}" class="form-control" type="text">
								</div>
							</div>
														
																					<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/work/outWork/edit.js">
	</script>
</body>
</html>
