<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header">
<style type="text/css">
input {
	readonly: readonly;
}
</style>
</head>

<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="id" name="id" th:value="${printPaper.id}"  type="hidden">
<div class="form-group">	
								<label class="col-sm-3 control-label">申请编号：</label>
								<div class="col-sm-8">
									<input id="applyNo" name="applyNo" readonly="readonly"  th:value="${printPaper.applyNo}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">申请人：</label>
								<div class="col-sm-8">
									<input id="applyId" readonly="readonly" name="applyId" th:value="${printPaper.applyId}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">项目名称：</label>
								<div class="col-sm-8">
									<input id="projectName" name="projectName" th:value="${printPaper.projectName}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">图纸名称：</label>
								<div class="col-sm-8">
									<input id="paperName" name="paperName" th:value="${printPaper.paperName}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">打图金额：</label>
								<div class="col-sm-8">
									<input id="printAmount" name="printAmount" th:value="${printPaper.printAmount}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">打图次数：</label>
								<div class="col-sm-8">
									<input id="printTimes" readonly="readonly" name="printTimes" th:value="${printPaper.printTimes}" class="form-control" type="text">
								</div>
							</div>
								
							<div class="panel panel-default" style="width: 71%;margin-left: 18%;">
						<div class="panel-heading">审核记录</div>
							  <table class="table">
							  	<thead>
							   		<tr>
							   			<td>序号</td>
							   			<td>审核日期</td>
							   			<td>审核节点</td>
							   			<td>审核人</td>
							   			<td>审核操作</td>
							   			<td>审核意见</td>
							   			
							   		</tr>
							   	</thead>
							   	<tbody>
							   	
							   		<tr th:if="${findAuditInfoList} ne null" th:each="obj,objsta:${findAuditInfoList}">
							   			<td th:text="${objsta.count}"></td>
							   			<td th:text="${#dates.format(obj.createDate,'yyyy-MM-dd')}"></td>
							   			<td th:text="${obj.taskName}"></td>
							   			<td th:text="${obj.auditUsername}"></td>
							   			<td th:text="${obj.auditOperateType}"></td>
							   			<td th:text="${obj.auditOpinion}"></td>
							   		</tr>
							   		<tr th:if="${findAuditInfoList} eq null">
							   			<td colspan="6">不存在审核数据</td>
							   		</tr>
							   	</tbody>	
							  </table>
					  </div>
							
							
							
							<div class="form-group" style="margin-top: 5px;">
                            <label class="col-sm-3 control-label">审核意见 ：</label>
                            <div class="col-sm-8">
                                <textarea id="taskComment" style="width: 95%;" width="95%" name="taskComment" 
                                          class="form-control"></textarea>
                            </div>
                        </div>
							<input type="hidden" name="auditMsg" id="auditMsg"  />
							<input type="hidden" th:value="${taskId}" name="taskId" />
							<input type="hidden" th:value="${processInstanceId}" name="processInstanceId"  />
							
														<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary" onclick="$('#auditMsg').val('1')">同意</button>
									<button type="submit" class="btn btn-primary" onclick="$('#auditMsg').val('0')">退回</button>
									
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/work/printPaper/editAudit.js">
	</script>
</body>
</html>
