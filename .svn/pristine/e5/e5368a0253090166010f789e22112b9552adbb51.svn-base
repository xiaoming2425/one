<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="id" name="id" th:value="${customer.id}"  type="hidden">
<div class="form-group">	
								<label class="col-sm-3 control-label">客户编号：</label>
								<div class="col-sm-8">
									<input id="customerNo" name="customerNo" th:value="${customer.customerNo}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">客户名称：</label>
								<div class="col-sm-8">
									<input id="customerName" name="customerName" th:value="${customer.customerName}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">客户类型：</label>
								<div class="col-sm-8">
									<!-- <input id="customerType" name="customerType" th:value="${customer.customerType}" class="form-control" type="text"> -->
								    <select   name="customerType" id="customerType" class="form-control">
									       <option  th:selected="${customer.customerType == '0'}" value="0">请选择</option>
									       <option   th:selected="${customer.customerType == '1'}"  value="1">安装公司</option>
									       <option   th:selected="${customer.customerType == '2'}"  value="2">设计院</option>
									       <option   th:selected="${customer.customerType == '3'}"  value="3">三产公司</option>
									</select>
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">客户经理：</label>
								<div class="col-sm-8">
									<input id="userId"  name="userId" th:value="${customer.suName}" readonly="readonly" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">所属机构：</label>
								<div class="col-sm-8">
									<input id="deptId"  name="deptId" th:value="${customer.sdName}" readonly="readonly"  class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">状态：</label>
								<div class="col-sm-8">
									<!-- <input id="type" name="type" th:value="${customer.type}" class="form-control" type="text"> -->
								    <select name="type" id="type" class="form-control">
									       <option th:selected="${customer.type == '0'}" value="0">请选择</option>
									       <option th:selected="${customer.type == '1'}" value="1">未成交</option>
									       <option th:selected="${customer.type == '2'}" value="2">已成交</option>
									</select>
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">客户来源：</label>
								<div class="col-sm-8">
									<!-- <input id="customerResource" name="customerResource" th:value="${customer.customerResource}" class="form-control" type="text"> -->
										<select name="customerResource" id="customerResource" class="form-control">
											       <option th:selected="${customer.customerResource == '0'}" value="0">请选择</option>
											       <option th:selected="${customer.customerResource == '1'}" value="1">领导推荐</option>
											       <option th:selected="${customer.customerResource == '2'}" value="2">陌生拜访</option>
											       <option th:selected="${customer.customerResource == '3'}" value="3">客户推荐</option>
										</select>
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">详细地址：</label>
								<div class="col-sm-8">
									<input id="detailAddress" name="detailAddress" th:value="${customer.detailAddress}" class="form-control" type="text">
								</div>
							</div>
																					<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
			 <div class="content_wrap" hidden="hidden">
						    <div class="zTreeDemoBackground left">
						        <ul id="treeDemo" class="ztree"></ul>
						    </div>
						</div>
						<div class="content_wrapT" id="content_wrap" hidden="hidden">
						    <div class="zTreeDemoBackground left">
						        <ul id="treeDemoT" class="ztree"></ul>
						    </div>
						</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
		<script type="text/javascript" src="/js/zTree3/jquery.ztree.core.js"></script>
	<script type="text/javascript" src="/js/zTree3/jquery.ztree.excheck.js"></script>
	<script type="text/javascript" src="/js/appjs/sale/customer/edit.js">
	</script>
</body>
</html>
