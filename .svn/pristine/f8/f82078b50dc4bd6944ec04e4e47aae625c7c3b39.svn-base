<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="id" name="id" th:value="${seqno.id}"  type="hidden">
<div class="form-group">	
								<label class="col-sm-3 control-label">编号类别：</label>
								<div class="col-sm-8">
									<input id="categoryCode" name="categoryCode" th:value="${seqno.categoryCode}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">编号类型：</label>
								<div class="col-sm-8">
									<input id="categoryName" name="categoryName" th:value="${seqno.categoryName}" class="form-control" type="text">
								</div>
							</div>
								
														
														<div class="form-group">	
								<label class="col-sm-3 control-label">规则：</label>
								<div class="col-sm-8">
									<input id="rule" name="rule" th:value="${seqno.rule}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">生成类型：</label>
								<div class="col-sm-8">
									<!-- <input id="genType" name="genType" th:value="${seqno.genType}" class="form-control" type="text"> -->
									<select name="genType" class="form-control">
											<option th:selected="${seqno.genType == '3'}" value="3">按年生成</option>
											<option th:selected="${seqno.genType == '2'}"  value="2">按月生成</option>
											<option th:selected="${seqno.genType == '1'}" value="1">按天生成</option>
									</select>
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">流水号长度：</label>
								<div class="col-sm-8">
									<input id="noLength" name="noLength" th:value="${seqno.noLength}" class="form-control" type="text">
								</div>
							</div>						
														<div class="form-group">	
								<label class="col-sm-3 control-label">初始值：</label>
								<div class="col-sm-8">
									<input id="initValue" name="initValue" th:value="${seqno.initValue}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">当前值：</label>
								<div class="col-sm-8">
									<input id="curValue" name="curValue" th:value="${seqno.curValue}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">步长：</label>
								<div class="col-sm-8">
									<input id="step" name="step" th:value="${seqno.step}" class="form-control" type="text">
								</div>
							</div>				
																					<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/common/seqno/edit.js">
	</script>
</body>
</html>
