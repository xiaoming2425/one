<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>

<body class="gray-bg">

	<div class="wrapper wrapper-content ">
	<div class="row">
		<div class="col-sm-11">
			<div class="ibox">
				<div class="ibox-body">
				
				
				      <ul class="nav nav-tabs">
							<li class="active"><a data-toggle="tab" href="#tab-1"><i class="fa fa-file-text"></i>项目列表</a>
							</li>
							<li class=""><a data-toggle="tab" href="#tab-3"><i class="fa fa-briefcase"></i>计划项目</a>
							</li>
							<li class=""><a data-toggle="tab" href="#tab-2"><i class="fa fa-briefcase"></i>计划列表</a>
							</li>
							
						</ul>
						<div class="tab-content">
							<div id="tab-1" class="tab-pane active">
								<div class="fixed-table-toolbar">
			
									<div class="columns pull-right">
										<button class="btn btn-success" onclick="reLoad()">查询</button>
									</div>
									<div class="columns pull-right col-md-2 nopadding">
										<input id="searchName" type="text" class="form-control"
											placeholder="请输入项目名称">
									</div>
								</div>
								<table id="exampleTable" data-mobile-responsive="true"></table>
							</div>
						
							<div id="tab-3" class="tab-pane ">
								<div class="fixed-table-toolbar">
			
									<div class="columns pull-right">
										<button class="btn btn-success" onclick="reLoadPlanList()">查询</button>
									</div>
									<div class="columns pull-right col-md-2 nopadding">
										<input id="searchNameList" type="text" class="form-control"
											placeholder="请输入项目名称">
									</div>
								</div>
								<table id="exampleTable3" data-mobile-responsive="true"></table>
							</div>
					
							<div id="tab-2" class="tab-pane ">
								<div class="fixed-table-toolbar">
			
									<div class="columns pull-right">
										<button class="btn btn-success" onclick="reLoadPlan()">查询</button>
									</div>
									<div class="columns pull-right col-md-2 nopadding">
										<input id="searchNameSingle" type="text" class="form-control"
											placeholder="请输入项目名称">
									</div>
								</div>
								<table id="exampleTable2" data-mobile-responsive="true"></table>
							</div>
						
						</div>

				</div>
			</div>
